    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-airbnb": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^7.20.0",
    "prettier": "2.4.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
