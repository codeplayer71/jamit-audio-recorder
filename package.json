{
  "name": "jamit-audio-recorder",
  "version": "0.1.6",
  "author": {
    "name": "Jasminko Milicevic",
    "email": "codeplayer71@gmail.com"
  },
  "description": "Nice little Vue Audio Recorder",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codeplayer71/jamit-audio-recorder.git"
  },
